@media only screen and (min-width : 767px)
	.full-conent
		max-width 1000px

	.header
		.icono-menu
			display none
		.nombre
			font-size 1.3em
			width 85%
	.content
		nav
			display block
			ul
				li
					width 18%
					height 35px
					list-style-type none
					display inline-block
					text-align center

	.footer-phone
		position absolute
		width 100%

	.Inicio-row
		display inline-block
		&-generacion
			width 25%
			display inline-block
			vertical-align top

		.img-head
			img
				width 300px
				height 34px
		.img-footer
			img
				width 300px
				height 34px
	.text-tradicion
		width 40%
		display inline-block
	.sugerencia
		width 40%
		float right

	.menu-historia-phone
		text-align center



	.submenu
		left 60%
